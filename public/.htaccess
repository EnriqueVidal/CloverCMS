PassengerEnabled on
PassengerAppRoot /home/cloverin/etc/rails_apps/clovercms/current
RailsEnv production

RewriteEngine on
RewriteCond %{HTTP_HOST} !^www\.
RewriteRule (.*) http://www.cloverinteractive.com/$1 [R=301,L]

Options -Indexes

<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif|js|css|swf)$">
	Header set Expires "Fri, 25 Dec 2010 00:00:00 GMT"
</FilesMatch>

<FilesMatch "\.(html|css|js)$">
  SetOutputFilter DEFLATE
</FilesMatch>

FileETag none
