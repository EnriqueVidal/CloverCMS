- if pages.count >= 1
  #column
    %h2.sb_header Our Services
    .sidebar
      %ul.services
        - pages.each do |page|
          %li= link_to page.title, show_section_page_path( :section_name => page.pageable.name, :page_name => page.name)
      %p.right= link_to "<span>More</span>", "#", :class => "button2"
    .sidebar_bottom
    