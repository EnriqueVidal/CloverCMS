- form_for(@contact_form) do |f|
  = f.error_messages
  %p
    = f.label :email
    %br
    = f.text_field :email
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :phone
    %br
    = f.text_field :phone
  %p
    = f.label :subject
    %br
    = f.text_field :subject
  %p
    = f.label :message
    %br
    = f.text_area :message
  %p
    = f.submit 'Send Email'